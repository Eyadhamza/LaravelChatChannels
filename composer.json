{
    "name": "theprofessor/laravel-chat-channels",
    "description": "A laravel package to create simple and very easy chat rooms and channels",
    "keywords": [
        "theprofessor",
        "Laravel-Chat-Channels"
    ],
    "homepage": "https://github.com/theprofessor/Laravel-Chat-Channels",
    "license": "MIT",
    "authors": [
        {
            "name": "EyadHamza",
            "email": "EyadHamza0@outlook.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "TheProfessor\\LaravelChatChannels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheProfessor\\LaravelChatChannels\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheProfessor\\LaravelChatChannels\\LaravelChatChannelsServiceProvider"
            ],
            "aliases": {
                "LaravelChatChannels": "TheProfessor\\LaravelChatChannels\\LaravelChatChannelsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
