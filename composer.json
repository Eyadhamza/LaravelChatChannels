{
    "name": "theprofessor/laravelchatchannels",
    "description": "a simple laravel package to create chats rooms and channels",
    "keywords": [
        "theprofessor",
        "laravelchatchannels"
    ],
    "homepage": "https://github.com/theprofessor/laravelchatchannels",
    "license": "MIT",
    "authors": [
        {
            "name": "EyadHamza",
            "email": "EyadHamza0@outlook.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-medialibrary": "^8.0.0"

    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "TheProfessor\\Laravelchatchannels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheProfessor\\Laravelchatchannels\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheProfessor\\Laravelchatchannels\\LaravelchatchannelsServiceProvider"
            ],
            "aliases": {
                "Laravelchatchannels": "TheProfessor\\Laravelchatchannels\\LaravelchatchannelsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
